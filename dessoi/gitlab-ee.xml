<?xml version="1.0"?>
<Container version="2">
  <Beta>False</Beta>
  <Category>Tools:</Category>
  <Date>2018-10-30</Date>
  <Name>GitLab-EE</Name>
  <Description>GitLab Enterprise Edition docker image based on the Omnibus package.</Description>
  <Overview>GitLab Enterprise Edition docker image based on the Omnibus package.</Overview>
  <Registry>https://hub.docker.com/r/gitlab/gitlab-ee/</Registry>
  <GitHub>https://github.com/gitlabhq/gitlabhq</GitHub>
  <Repository>gitlab/gitlab-ee</Repository>
  <ExtraParams>--env GITLAB_OMNIBUS_CONFIG="external_url 'http://unraid:9080/'"</ExtraParams>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
    <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>9080</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>9443</HostPort>
        <ContainerPort>443</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>9022</HostPort>
        <ContainerPort>22</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/gitlab-ee/config</HostDir>
      <ContainerDir>/etc/gitlab</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/appdata/gitlab-ee/data</HostDir>
      <ContainerDir>/var/opt/gitlab</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/appdata/gitlab-ee/log</HostDir>
      <ContainerDir>/var/log/gitlab</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment/>
  <WebUI>http://[IP]:[PORT:9080]</WebUI>
  <Banner>https://raw.githubusercontent.com/dessoi/docker-templates/master/images/gitlab.png</Banner>
  <Icon>https://raw.githubusercontent.com/dessoi/docker-templates/master/images/gitlab_small.png</Icon>
  <Network Default="bridge">bridge</Network>
  <Config Type="Port" Name="Web Interface Port (HTTP)" Target="9080" Default="9080" Mode="tcp" Display="always-hide" Required="true" Description="Web UI port.">9080</Config>
  <Config Type="Port" Name="Web Interface Port (HTTPS)" Target="9443" Default="9443" Mode="tcp" Display="always-hide" Required="true" Description="SSL Web UI port.">9443</Config>
  <Config Type="Port" Name="SSH Port" Target="9022" Default="9022" Mode="tcp" Display="always-hide" Required="true" Description="SSH port.">9022</Config>
  <Config Type="Path" Name="Config Storage Path" Target="/etc/gitlab" Default="/mnt/cache/appdata/gitlab-ee/config" Mode="rw" Display="advanced-hide" Required="true" Description="GitLab configuration files.">/mnt/cache/appdata/gitlab-ee/config</Config>
  <Config Type="Path" Name="Application Data Storage Path" Target="/var/opt/gitlab" Default="/mnt/cache/appdata/gitlab-ee/data" Mode="rw" Display="advanced-hide" Required="true" Description="GitLab application data.">/mnt/cache/appdata/gitlab-ee/data</Config>
  <Config Type="Path" Name="Log Storage Path" Target="/var/log/gitlab" Default="/mnt/cache/appdata/gitlab-ee/logs" Mode="rw" Display="advanced-hide" Required="true" Description="GitLab logs.">/mnt/cache/appdata/gitlab-ee/logs</Config>
</Container>